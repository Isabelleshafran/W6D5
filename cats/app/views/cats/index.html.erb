<h1>All Cats</h1>

<ul>
<% @cats.each do |cat| %>
    <li>
        <ul>
            <li><%= cat.name %></li>
            <li><%= cat.color %></li>
            <li><%= cat.birth_date %></li>
            <li><%= cat.age %></li>
            <li><%= cat.sex %></li>
        </ul>
    </li>
    <!-- <li><%#= render 'cat', cat: cat, action: 'index' %></li> -->
<% end %>
</ul>

